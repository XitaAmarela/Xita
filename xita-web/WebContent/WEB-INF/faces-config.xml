<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
	version="2.2">

	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>principal</from-outcome>
			<to-view-id>/pages/principal.jsf</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>detalhesProduto</from-outcome>
			<to-view-id>/pages/detalhesProduto.jsf</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>cadastroCliente</from-outcome>
			<to-view-id>/pages/cadastroCliente.jsf</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>cadastroOfertante</from-outcome>
			<to-view-id>/pages/cadastroOfertante.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>cadastroCliente</from-outcome>
			<to-view-id>/pages/cadastroCliente.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

</faces-config>
